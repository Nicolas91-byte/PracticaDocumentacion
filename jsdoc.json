{
  "opts": {
    "template": "node_modules/daybrush-jsdoc-template"
  },
  "source": {
    "include": ["./js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
